   "source": [
    "valid_mask = (corine_10m > 0) & study_area_mask\n",
    "void_valid_mask = valid_mask & void_mask\n",
    "\n",
    "codes = corine_10m[valid_mask].astype(np.int64)\n",
    "total_counts = np.bincount(codes)\n",
    "\n",
    "void_counts = np.bincount(\n",
    "    corine_10m[void_valid_mask].astype(np.int64),\n",
    "    minlength=total_counts.size\n",
    ")\n",
    "\n",
    "lc_codes = np.nonzero(total_counts)[0]\n",
    "total_counts = total_counts[lc_codes]\n",
    "void_counts = void_counts[lc_codes]\n",
    "\n",
    "\n",
    "def _corine_color_hex(code: int) -> str:\n",
    "    rgb = CORINE_COLORS.get(int(code))\n",
    "    if rgb is None:\n",
    "        return '#999999'\n",
    "    return '#{0:02x}{1:02x}{2:02x}'.format(*rgb)\n",
    "\n",
    "\n",
    "def _corine_color_rgb(code: int):\n",
    "    rgb = CORINE_COLORS.get(int(code))\n",
    "    if rgb is None:\n",
    "        return (0.6, 0.6, 0.6)\n",
    "    return tuple(channel / 255 for channel in rgb)\n",
    "\n",
    "\n",
    "void_lc_df = pd.DataFrame({\n",
    "    'code': lc_codes.astype(int),\n",
    "    'name': [CORINE_CLASSES.get(int(code), f'Class {int(code)}') for code in lc_codes],\n",
    "    'total_pixels': total_counts.astype(np.int64),\n",
    "    'void_pixels': void_counts.astype(np.int64),\n",
    "})\n",
    "\n",
    "void_lc_df['void_pct'] = np.where(\n",
    "    void_lc_df['total_pixels'] > 0,\n",
    "    100.0 * void_lc_df['void_pixels'] / void_lc_df['total_pixels'],\n",
    "    0.0\n",
    ")\n",
    "void_lc_df['valid_pct'] = 100.0 - void_lc_df['void_pct']\n",
    "void_lc_df['area_km2'] = void_lc_df['total_pixels'] * (resolution ** 2) / 1e6\n",
    "void_lc_df['color'] = [_corine_color_hex(code) for code in void_lc_df['code']]\n",
    "void_lc_df['color_rgb'] = [_corine_color_rgb(code) for code in void_lc_df['code']]\n",
    "\n",
    "void_lc_df = (\n",
    "    void_lc_df[void_lc_df['void_pixels'] > 0]\n",
    "    .sort_values('void_pct', ascending=False)\n",
    "    .reset_index(drop=True)\n",
    ")\n",
    "void_lc_stats = void_lc_df\n",
    "\n",
    "void_lc_summary = void_lc_df[\n",
    "    ['code', 'name', 'void_pixels', 'void_pct', 'valid_pct', 'area_km2']\n",
    "]\n",
    "void_lc_summary\n",
    "\n",
    "if void_lc_df.empty:\n",
    "    print('No void pixels found within the study area.')\n",
    "else:\n",
    "    max_code = int(void_lc_df['code'].max()) + 1\n",
    "    color_lut = np.full((max_code, 3), 0.75, dtype=float)\n",
    "    for code, rgb in zip(void_lc_df['code'], void_lc_df['color_rgb']):\n",
    "        if code < max_code:\n",
    "            color_lut[int(code)] = rgb\n",
    "\n",
    "    void_rgb = np.ones((*corine_10m.shape, 3), dtype=float)\n",
    "    void_rgb[void_valid_mask] = color_lut[corine_10m[void_valid_mask].astype(int)]\n",
    "\n",
    "    fig, axes = plt.subplots(1, 2, figsize=(18, 8))\n",
    "\n",
    "    axes[0].imshow(void_rgb)\n",
    "    axes[0].set_title('Void zones by CORINE class')\n",
    "    axes[0].axis('off')\n",
    "\n",
    "    legend_handles = [\n",
    "        Patch(facecolor=color, edgecolor='black', label=name)\n",
    "        for color, name in zip(void_lc_df['color'], void_lc_df['name'])\n",
    "    ]\n",
    "    axes[0].legend(\n",
    "        handles=legend_handles,\n",
    "        loc='upper left',\n",
    "        bbox_to_anchor=(1.02, 1),\n",
    "        frameon=False,\n",
    "        title='Land cover'\n",
    "    )\n",
    "\n",
    "    bar_data = void_lc_df.sort_values('void_pct', ascending=True)\n",
    "    axes[1].barh(bar_data['name'], bar_data['void_pct'], color=bar_data['color'])\n",
    "    axes[1].set_xlabel('Void percentage (%)')\n",
    "    axes[1].set_title('Void share by land cover class')\n",
    "\n",
    "    max_void_pct = bar_data['void_pct'].max()\n",
    "    axes[1].set_xlim(0, max(5, max_void_pct * 1.1))\n",
    "    label_offset = max(0.5, max_void_pct * 0.01)\n",
    "\n",
    "    for idx, value in enumerate(bar_data['void_pct']):\n",
    "        axes[1].text(value + label_offset, idx, f'{value:.1f}%', va='center')\n",
    "\n",
    "    plt.tight_layout()\n"
